{
  "src": [
    "./index.js",
    "./src/*.js",
    "./README.md"
  ],
  "dest": "./docs",
  "jsdoc": {
    "hierarchy": true,
    "sort": "kind",
    "plugins": [
      "plugins/markdown"
    ]
  },
  "markdown": {
    "sanitize": false
  },
  "app": {
    "title": "Kazagumo",
    "routing": "query",
    "entrance": "content:readme",
    "base": "/Kazagumo"
  },
  "template": {
    "options": {
      "title": {
        "label": "Kazagumo",
        "href": "."
      },
      "sidebar": {
        "enabled": true,
        "outline": "tree",
        "collapsed": false,
        "toolbar": true,
        "itemsFolded": true,
        "itemsOverflow": "crop",
        "badges": true,
        "search": true,
        "animations": true
      },
      "symbols": {
        "autoLink": true,
        "params": "list",
        "enums": "list",
        "props": "list",
        "meta": true
      },
      "navbar": {
        "enabled": true,
        "dark": true,
        "animations": true,
        "menu": [
          {
            "label": "Readme",
            "href": "."
          },
          {
            "label": "Documentation",
            "href": "?api"
          },
          {
            "label": "GitHub",
            "href": "https://github.com/Takiyo0/Kazagumo"
          }
        ]
      }
    }
  }
}